{
  "job_name": "sftp_csv",
  "source_type": "sftp",
  "ingestion_type": "incremental",
  "ingest_method": "paramiko", 
  "incremental_column": "timestamp",
  "last_ingestion_value": "2025-01-01 00:00:00",
  "source_config": {
    "host": "sftp_server",
    "port": 22,
    "username": "user",
    "password": "pass",
    "file_path": "upload/data_sample.csv",
    "file_type": "csv"
  },
  "target": {
    "format": "delta",
    "path": "s3a://raw-data/sftp_csv_data",
    "primary_key": "id",
    "transformations": [
      { "type": "fill_na", "columns": ["price"], "value": 0 },
      { "type": "upper_case", "columns": ["product", "category"] },
      { "type": "add_ingestion_timestamp" }
    ]
  },
  "dag_params": {
    "schedule_interval": "40 * * * *",
    "owner": "data_engine",
    "retries": 1,
    "retry_delay_min": 5,
    "start_date": "2025-01-01",
    "catchup": false
  }
}