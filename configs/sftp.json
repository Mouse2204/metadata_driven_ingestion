{
  "job_name": "ingest_sftp_csv",
  "source_type": "sftp",
  "source_config": {
    "host": "sftp_server",
    "port": 22,
    "username": "user",
    "password": "pass",
    "file_path": "upload/data_sample.csv",
    "file_type": "csv"
  },
    "target": {
    "format": "delta",
    "path": "s3a://raw-data/sftp_csv_data",
    "primary_key": "id",
    "transformations": [
      { "type": "fill_na", "columns": ["price"], "value": 0 },
      { "type": "upper_case", "columns": ["product", "category"] },
      { "type": "add_ingestion_timestamp" }
    ]
  }
}